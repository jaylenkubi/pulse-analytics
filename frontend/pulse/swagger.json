    "/analytics/stats": {
      "get": {
        "operationId": "AnalyticsController_getEventStats",
        "parameters": [
          {
            "name": "start",
            "required": true,
            "in": "query",
            "schema": {
              "example": "2024-03-01T00:00:00Z",
              "type": "string"
            }
          },
          {
            "name": "end",
            "required": true,
            "in": "query",
            "schema": {
              "example": "2024-03-02T00:00:00Z",
              "type": "string"
            }
          }
