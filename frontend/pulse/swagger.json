    "/analytics/timeline": {
      "get": {
        "operationId": "AnalyticsController_getEventTimeline",
        "parameters": [
          {
            "name": "start",
            "required": true,
            "in": "query",
            "schema": {
              "example": "2024-03-01T00:00:00Z",
              "type": "string"
            }
          },
          {
            "name": "end",
            "required": true,
            "in": "query",
            "schema": {
              "example": "2024-03-02T00:00:00Z",
              "type": "string"
            }
          },
          {
            "name": "eventType",
            "required": true,
            "in": "query",
            "schema": {
              "example": "pageView",
              "type": "string"
            }
          },
          {
            "name": "interval",
            "required": true,
            "in": "query",
            "schema": {
              "enum": [
                "1m",
                "5m",
                "1h"
              ],
              "type": "string"
            }
          }
        ],
        "responses": {
          "200": {
            "description": ""
          }
        },
        "summary": "Get event timeline",
        "tags": [
          "Analytics"
        ]
      }
